his.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    goTo(url) {\n      this.$router.push(url);\n    },\n    del(id) {\n      this.$request.delete('/address/delete/' + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success('删除成功');\n          this.loadAddress();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.loadAddress();\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.loadAddress();\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","addressData","pageNum","pageSize","total","formVisible","form","rules","username","required","message","trigger","useraddress","phone","mounted","loadAddress","methods","addAddress","editAddress","row","stringify","save","$refs","formRef","validate","valid","userId","id","$request","url","method","then","res","code","$message","success","error","msg","get","params","list","goTo","$router","push","del","delete","handleCurrentChange","handleSizeChange"],"sources":["src/views/front/Address.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"flex: 5; height: 60px; background-color: #81d7ce\"></div>\r\n    \r\n    <div style=\"display: flex; \">\r\n      <div class=\"left\"></div>\r\n      <div style=\"width: 66%;  height: 1000px; border-radius: 15px; background-color: white\">\r\n        <div style=\"display: flex; width: 80%; margin: 30px auto;\">\r\n          <div style=\" flex: 1\">\r\n            <div style=\"display: flex; font-size: 18px; color: #000000FF; line-height: 80px; border-bottom: #cccccc 1px solid;\">\r\n              <div style=\"flex: 3;\">我的地址</div>\r\n              <div style=\"flex: 1; text-align: right;\">\r\n                <el-button type=\"warning\" round @click=\"addAddress\">添加收货地址</el-button>\r\n              </div>\r\n            </div>\r\n            <div style=\"margin: 20px 0; \">\r\n              <div class=\"table\">\r\n                <el-table :data=\"addressData\" strip>\r\n                  <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n                  <el-table-column prop=\"username\" label=\"收货人\" width=\"100px\"></el-table-column>\r\n                  <el-table-column prop=\"useraddress\" label=\"收货地址\"></el-table-column>\r\n                  <el-table-column prop=\"phone\" label=\"联系电话\"></el-table-column>\r\n                  <el-table-column label=\"操作\" align=\"center\" width=\"180\">\r\n                    <template v-slot=\"scope\">\r\n                      <el-button size=\"mini\" type=\"primary\" plain @click=\"editAddress(scope.row)\">编辑</el-button>\r\n                      <el-button size=\"mini\" type=\"danger\" plain @click=\"del(scope.row.id)\">删除</el-button>\r\n                    </template>\r\n                  </el-table-column>\r\n                </el-table>\r\n\r\n                <div class=\"pagination\" style=\"margin-top: 20px\">\r\n                  <el-pagination\r\n                      @size-change=\"handleSizeChange\"\r\n                      @current-change=\"handleCurrentChange\"\r\n                      :current-page=\"pageNum\"\r\n                      :page-sizes=\"[5, 10, 20]\"\r\n                      :page-size=\"pageSize\"\r\n                      layout=\"total, sizes, prev, pager, next, jumper\"\r\n                      :total=\"total\">\r\n                  </el-pagination>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <el-dialog title=\"地址信息\" :visible.sync=\"formVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n        <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" :rules=\"rules\" ref=\"form